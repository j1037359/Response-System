V     \uacfc\ubaa9\uc758 \ud615\ud0dc\uc5d0 \ub530\ub77c \ucd9c\uc11d\ud654\uba74\uc774 \ub2ec\ub77c\uc9d1\ub2c8\ub2e4. \uacfc\ubaa9\uc758 \ud615\ud0dc\ub85c\ub294 e-Class(\uc774\ud074\ub798\uc2a4)\uc640 e-Learning(\uc774\ub7ec\ub2dd)\uc774 \uc788\uc2b5\ub2c8\ub2e4.* e-Class(\uc774\ud074\ub798\uc2a4) : \uc624\ud504\ub77c\uc778 \uac15\uc758\ub97c \uc9c0\uc6d0\ud569\ub2c8\ub2e4.* e-Learning(\uc774\ub7ec\ub2dd) : \uc628\ub77c\uc778\uc73c\ub85c \uc9c4\ud589\ub418\ub294 \uac15\uc758(\uc778\ud130\ub137 \uc218\uc5c5)\ub97c \uc9c0\uc6d0\ud569\ub2c8\ub2e4.[\uc77c\ubc18\ucd9c\uc11d]\uc77c\ubc18\ucd9c\uc11d\uc740 e-Class(\uc774\ud074\ub798\uc2a4) \ud615\ud0dc\uc758 \uacfc\ubaa9\uc5d0\uc11c \uc774\uc6a9\uac00\ub2a5\ud569\ub2c8\ub2e4.\uc77c\ubc18\ucd9c\uc11d\uc740 \uc624\ud504\ub77c\uc778 \uac15\uc758\uc2e4\uc5d0\uc11c \uc218\uac15\uc0dd\uc758 \uc774\ub984\uc744 \ubd88\ub7ec \ucd9c\uc11d\uc744 \ud655\uc778\ud558\ub294 \uc77c\ubc18\uc801\uc778 \ubc29\uc2dd\uc785\ub2c8\ub2e4.[\uc77c\ubc18\ucd9c\uc11d \ub4f1\ub85d]1. '\ucd9c\uc11d' \uba54\ub274\ub85c \uc774\ub3d9\ud55c \ud6c4 '\ubaa9\ub85d' \ud0ed\uc5d0\uc11c '\uc77c\ubc18\ucd9c\uc11d \ub4f1\ub85d' \ubc84\ud2bc\uc744 \ud074\ub9ad\ud569\ub2c8\ub2e4. \uc77c\ubc18\ucd9c\uc11d \ub4f1\ub85d\ud654\uba74\uc73c\ub85c \uc774\ub3d9\ud569\ub2c8\ub2e4.2. \ud654\uba74\uc5d0 \uc218\uac15\uc0dd\ubaa9\ub85d\uc774 \ud45c\uc2dc\ub429\ub2c8\ub2e4. \uc218\uac15\uc0dd\uc758 \uc774\ub984\uc744 \ud074\ub9ad\ud558\uba74 \uc218\uac15\uc0dd\uc758 \uc815\ubcf4\uac00 \ub808\uc774\uc5b4 \ud31d\uc5c5\uc73c\ub85c \ub098\ud0c0\ub0a9\ub2c8\ub2e4.\uc218\uac15\uc0dd \ubaa9\ub85d\uc744 \uc815\ub82c\ud558\ub824\uba74 \uc815\ub82c\uc744 \uc124\uc815\ud558\uc138\uc694. \uc815\ub82c\uc758 \uac01 \ubc84\ud2bc\uc740 \uc774\ub984, \ud559\ubc88, \ud559\uacfc, \ud559\ub144 \uc21c\uc11c\ub85c \uc815\ub82c\ub418\ub294 \uac83\uc744 \ub098\ud0c0\ub0c5\ub2c8\ub2e4.\uc815\ub82c \uc21c\uc11c\ub97c \ubc14\uafb8\ub824\uba74 \ud574\ub2f9 \ud56d\ubaa9\uc744 \ud074\ub9ad\ud55c \ud6c4 \uc6d0\ud558\ub294 \uc704\uce58\ub85c \uc774\ub3d9(Drag & Drop)\uc2dc\ud0b5\ub2c8\ub2e4.\uc55e\ucabd(\uc67c\ucabd)\uc5d0 \uc704\uce58\ud560\uc218\ub85d \uc815\ub82c \uc21c\uc11c\uc5d0 \ub354 \uc601\ud5a5\uc744 \ubbf8\uce69\ub2c8\ub2e4. \ud56d\ubaa9\uc744 \ub354\ube14\ud074\ub9ad\ud558\uba74 \uc624\ub984\ucc28\uc21c \ub610\ub294 \ub0b4\ub9bc\ucc28\uc21c\uc73c\ub85c \ubcc0\uacbd\ub429\ub2c8\ub2e4.\uc815\ub82c \uc21c\uc11c\uc640 \uc815\ub82c \ubc29\uc2dd(\uc624\ub984\ucc28\uc21c, \ub0b4\ub9bc\ucc28\uc21c)\uc744 \ubcc0\uacbd\ud558\uc2e0 \ub4a4 'Search' \ubc84\ud2bc\uc744 \ud074\ub9ad\ud558\uc138\uc694. \uc815\ub82c \uc870\uac74\uc5d0 \ub9de\uac8c \uc218\uac15\uc0dd\uc774 \uc870\ud68c\ub429\ub2c8\ub2e4.\ub0a0\uc9dc \uc6b0\uce21\uc758 \uce78\uc744 \ud074\ub9ad\ud558\uba74 \ub2ec\ub825\uc774 \ub098\ud0c0\ub0a9\ub2c8\ub2e4. \ub2ec\ub825\uc5d0\uc11c \ucd9c\uc11d\uc77c\uc790\ub97c \uc120\ud0dd\ud558\uc138\uc694.\uacfc\ubaa9\uc2dc\uac04\uc740 \uad50\uc218\ub2d8\uc758 \uad6c\ubd84\uc744 \uc704\ud55c \uac12\uc73c\ub85c \ud574\ub2f9 \uacfc\ubaa9\uc758 \uad50\uc2dc\uc2dc\uac04\uc73c\ub85c \uc124\uc815\ud558\uc138\uc694.Ex ) \uad50\uc218\ub2d8\uaed8\uc11c 2~3\uad50\uc2dc\uc758 \uc218\uc5c5\uc744 \uc9c4\ud589\ud558\uace0 \ud55c \uad50\uc2dc\ub2f9 50\ubd84\uc758 \uc218\uc5c5\uc77c \uacbd\uc6b0 \uacfc\ubaa9\uc2dc\uac04\uc740 2\uc774\uace0, \ucd1d \ud559\uc2b5\uc2dc\uac04\uc740 100\ubd84\uc785\ub2c8\ub2e4.\uc774\ub54c\uc758 \uad50\uc2dc\uac1c\uc218\uc778 2\uac00 \uacfc\ubaa9\uc2dc\uac04\uc785\ub2c8\ub2e4. \uc218\uac15\uc0dd \ubaa9\ub85d\uc5d0\uc11c \ub77c\ub514\uc624 \ubc84\ud2bc\uc73c\ub85c \uc218\uac15\uc0dd\uc758 \ucd9c\uc11d\uc0c1\ud0dc\ub97c \uc124\uc815\ud558\uc138\uc694.\ube68\uac04\uc0c9 \ud14c\ub450\ub9ac \ub0b4 \uac01\uac01\uc758 \ucd9c\uc11d\uc0c1\ud0dc(\ucd9c\uc11d, \uc9c0\uac01, \uacb0\uc11d)\ub97c \uc120\ud0dd\ud558\uba74 \ubaa8\ub4e0 \uc218\uac15\uc0dd\uc758 \ucd9c\uc11d\uc0c1\ud0dc\uac00 \uc120\ud0dd\ud55c \ucd9c\uc11d\uc0c1\ud0dc\ub85c \ubcc0\uacbd\ub429\ub2c8\ub2e4.\uc218\uac15\uc0dd\uc758 \ucd9c\uc11d\uc0c1\ud0dc\ub97c \uc124\uc815\ud558\uc2e0 \ub4a4 '\uc800\uc7a5' \ubc84\ud2bc\uc744 \ud074\ub9ad\ud558\uba74 \uc120\ud0dd\ub41c \ub0a0\uc9dc\uc5d0 \ucd9c\uc11d\uc774 \uc800\uc7a5\ub429\ub2c8\ub2e4.[\uc77c\ubc18\ucd9c\uc11d \uc870\ud68c \ubc0f \uc218\uc815, \uc0ad\uc81c]1. '\ucd9c\uc11d' \uba54\ub274\ub85c \uc774\ub3d9\ud55c \ud6c4 '\ubaa9\ub85d' \ud0ed\uc5d0\uc11c \ucd9c\uc11d\ub0b4\uc6a9\uc744 \uc870\ud68c\ud560 \ucd9c\uc11d\uc77c\uc744 \ud074\ub9ad\ud558\uc138\uc694. \uc77c\ubc18\ucd9c\uc11d \uc870\ud68c\ud654\uba74\uc73c\ub85c \uc774\ub3d9\ud569\ub2c8\ub2e4.2. \ud574\ub2f9\uc77c\uc790\uc758 \uc77c\ubc18\ucd9c\uc11d\uc774 \uc870\ud68c\ub429\ub2c8\ub2e4. \uc218\uac15\uc0dd\uc758 \uc774\ub984\uc744 \ud074\ub9ad\ud558\uba74 \uc218\uac15\uc0dd\uc758 \uc815\ubcf4\uac00 \ub808\uc774\uc5b4 \ud31d\uc5c5\uc73c\ub85c \ub098\ud0c0\ub0a9\ub2c8\ub2e4.3. \uc218\uac15\uc0dd\uc758 \ucd9c\uc11d\uc0c1\ud0dc\ub97c \ubcc0\uacbd\ud55c \ub4a4 '\uc218\uc815' \ubc84\ud2bc\uc744 \ud074\ub9ad\ud558\uba74 \ubcc0\uacbd\ub41c \ub0b4\uc6a9\uc774 \uc800\uc7a5\ub429\ub2c8\ub2e4.'\uc0ad\uc81c' \ubc84\ud2bc\uc744 \ud074\ub9ad\ud558\uba74 \ud574\ub2f9\uc77c\uc790\uc758 \uc77c\ubc18\ucd9c\uc11d\uc774 \uc0ad\uc81c\ub429\ub2c8\ub2e4. 
