V \ud06c\ub86c\uc774\ub098 \ud30c\uc774\uc5b4\ud3ed\uc2a4 \ub4f1 \ub2e4\ub978 \ube0c\ub77c\uc6b0\uc800\ub85c \uc811\uc18d\ud558\uba74 \uc798 \ub418\ub294\ub370 \uc778\ud130\ub137 \uc775\uc2a4\ud50c\ub85c\ub7ec\ub85c \uc811\uc18d\ud588\uc744 \ub54c \uc2a4\ud06c\ub864 \ubc14\uac00 \ubcf4\uc774\uc9c0 \uc54a\uac70\ub098, \ud654\uba74\uc774 \ud2c0\uc5b4\uc9c0\uac70\ub098, \uc0ac\uc774\ud2b8 \uae30\ub2a5\uc774 \uc798 \uc548\ub41c\ub2e4\uba74\uc778\ud130\ub137 \uc775\uc2a4\ud50c\ub85c\ub7ec\uc758 \uc124\uc815\uc744 \ucd08\uae30\ud654 \ud574\uc8fc\uba74 \ub429\ub2c8\ub2e4.1. \uba3c\uc800, \uc5f4\ub824\uc788\ub294 \uc778\ud130\ub137 \uc775\uc2a4\ud50c\ub85c\ub7ec\ub97c \ubaa8\ub450 \ub2eb\uc740 \ud6c4 \uc778\ud130\ub137 \uc775\uc2a4\ud50c\ub85c\ub7ec \ucc3d\uc744 \ud558\ub098\ub9cc \ub744\uc6cc\uc8fc\uc138\uc694.\uc8fc\uc758! \uc5ec\ub7ec \uac1c\uc758 \uc778\ud130\ub137 \uc775\uc2a4\ud50c\ub85c\ub7ec\uac00 \uc5f4\ub824\uc788\uc73c\uba74 \uc124\uc815\uc774 \uc81c\ub300\ub85c \ucd08\uae30\ud654\ub418\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4.2. \uc778\ud130\ub137 \uc635\uc158\uc744 \uc5f0\ub2e4.- 'Alt' \ud0a4 -> '\ub3c4\uad6c' -> '\uc778\ud130\ub137 \uc635\uc158' \ud074\ub9ad3. '\uace0\uae09' \ud0ed\uc73c\ub85c \uc774\ub3d9 \ud6c4 '\uc6d0\ub798\ub300\ub85c' \ud074\ub9ad4. '\ub2e4\uc2dc \uc124\uc815' \ud074\ub9ad5. \uc124\uc815\uc774 \uc644\ub8cc\ub418\uba74 \uc778\ud130\ub137 \uc775\uc2a4\ud50c\ub85c\ub7ec\ub97c \ubaa8\ub450 \uc885\ub8cc\ud55c \ud6c4 \ub2e4\uc2dc \uc0ac\uc774\ud2b8\uc5d0 \uc811\uc18d\ud558\uc5ec \uc124\uc815\uc774 \ucd08\uae30\ud654\ub41c \uac83\uc744 \ud655\uc778\ud569\ub2c8\ub2e4. 
